{
  "name": "rosammen",
  "version": "1.0.0",
  "repository": "git@github.com:bardrotzer/rosammen.git",
  "author": "Bård Røtzer <bard@kartoteket.as>",
  "license": "MIT",
  "devDependencies": {
    "svelte": "^2.16.1",
    "tailwindcss": "^0.7.4",
    "webpack": "^4.29.6"
  },
  "scripts": {
    "build": "rollup -c",
    "autobuild": "yarn tailwind && rollup -c -w",
    "dev": "run-p start:dev autobuild",
    "start": "sirv public",
    "start:dev": "sirv public --dev",
    "tailwind": "yarn tailwind:normalize && yarn tailwind:css",
    "tailwind:normalize": "tailwind build src/css/normalize.src.css -c tailwind.js -o src/css/normalize.css",
    "tailwind:css": "tailwind build src/css/tailwind.src.css -c tailwind.js -o src/css/tailwind.css"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "leaflet": "^1.4.0"
  }
}
