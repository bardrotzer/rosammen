{
  "name": "rosammen",
  "version": "1.0.0",
  "repository": "git@github.com:bardrotzer/rosammen.git",
  "author": "Bård Røtzer <bard@kartoteket.as>",
  "license": "MIT",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "rollup": "^1.2.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-purgecss": "^1.0.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^4.0.4",
    "sirv-cli": "^0.2.3",
    "svelte": "^2.16.1",
    "tailwindcss": "^0.7.4"
  },
  "scripts": {
    "build": "rollup -c",
    "autobuild": "yarn tailwind && rollup -c -w",
    "dev": "run-p start:dev autobuild",
    "start": "sirv public",
    "start:dev": "sirv public --dev",
    "tailwind": "yarn tailwind:normalize && yarn tailwind:css",
    "tailwind:normalize": "tailwind build src/css/normalize.src.css -c tailwind.js -o src/css/normalize.css",
    "tailwind:css": "tailwind build src/css/tailwind.src.css -c tailwind.js -o src/css/tailwind.css"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "leaflet": "^1.4.0"
  }
}
