  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^9.4.10",
    "copy-webpack-plugin": "^5.0.0",
    "css-loader": "^2.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss-cli": "^6.1.2",
    "postcss-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "svelte": "^2.16.1",
    "svelte-loader": "^2.13.3",
    "tailwindcss": "^0.7.4",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "scripts": {
    "build": "yarn tailwind && yarn webpack --mode production",
    "dev": "yarn webpack-dev-server --mode development",
    "start": "sirv public",
    "start:dev": "sirv public --dev",
    "tailwind": "yarn tailwind:normalize && yarn tailwind:css",
    "tailwind:normalize": "tailwind build src/css/normalize.css.src -c tailwind.config.js -o src/css/normalize.css",
    "tailwind:css": "tailwind build src/css/tailwind.css.src -c tailwind.config.js -o src/css/tailwind.css"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "leaflet": "^1.4.0"
  }
}
